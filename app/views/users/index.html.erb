<div class="container">
  <div class="row">

    <%= render 'user_info', book: @book %>

    <div class="col">
      <h2>Users</h2>
      <table class="table">
        <thead>
          <tr>
            <th>image</th>
            <th>name</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
          <tr>
            <td></td>
            <td><%= user.name %></td>
            <td><%= link_to "Show", user_path(user.id)%></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>